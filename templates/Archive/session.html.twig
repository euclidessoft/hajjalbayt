{% extends "MecqueBundle::layout.html.twig" %}
{% block home %} class="active" {% endblock %}
{% block body %}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">{{ session }}</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row" style="padding-top: 55px;">
    <div class="col-lg-2 col-lg-offset-2 col-md-5">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-12">
                        <i class="fa fa-users fa-5x"></i>
                    </div>
                </div>
                <div class="row">

                    <div class="col-xs-12 text-right">
                        <div class="huge">{{ nombre}}</div>
                        <div>Pelerin(s) Enregistr&eacute;(s)
                        </div>
                    </div>
                </div>
            </div>
            <a href="#">
                <div class="panel-footer">
                    <span class="pull-left"><a href="{{ path('Mecque_ArchivePelerinList', { saison : session }) }}">Voir Details</a></span>
                    <span class="pull-right"><a href="{{ path('Mecque_ArchivePelerinList', { saison : session }) }}"><i class="fa fa-arrow-circle-right"></i></a></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-2 col-md-5">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-12">
                        <i class="fa fa-male fa-5x"></i>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 text-right">
                        <div class="huge">{{ enregle }}</div>
                        <div>Hommes<br />&nbsp;</div>
                    </div>
                </div>
            </div>
            <a href="#">
                <div class="panel-footer">
                    <span class="pull-left"><a href="{{ path('Mecque_ArchivePelerinListMale', { saison : session }) }}">Voir Details</a></span>
                    <span class="pull-right"><a href="{{ path('Mecque_ArchivePelerinListMale', { saison : session }) }}"><i class="fa fa-arrow-circle-right"></i></a></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-2 col-md-6">
        <div class="panel panel-warning">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-12">
                        <i class="fa fa-female fa-5x"></i>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 text-right">
                        <div class="huge">{{ pasenregle }}</div>
                        <div>Femmes<br />&nbsp;</div>
                    </div>
                </div>
            </div>
            <a href="#">
                <div class="panel-footer">
                    <span class="pull-left"><a href="{{ path('Mecque_ArchivePelerinListFemale', { saison : session }) }}">Voir Details</a></span>
                    <span class="pull-right"><a href="{{ path('Mecque_ArchivePelerinListFemale', { saison : session }) }}"><i class="fa fa-arrow-circle-right"></i></a></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-2 col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-12">
                        <i class="fa fa-folder-open-o fa-5x"></i>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12 text-right">
                        <div class="huge">{{ dossier }}</div>
                        <div>Abandon(s)<br />&nbsp;</div>
                    </div>
                </div>
            </div>
            <a href="#">
                <div class="panel-footer">
                    <span class="pull-left">
                        <a href="{{ path('Mecque_ArchivePelerinOldList', { saison : session }) }}">
                            View Details
                        </a>
                    </span>
                    <span class="pull-right"><a href="{{ path('Mecque_ArchivePelerinOldList', { saison : session }) }}"><i class="fa fa-arrow-circle-right"></i></a></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
</div>
{% endblock%}
